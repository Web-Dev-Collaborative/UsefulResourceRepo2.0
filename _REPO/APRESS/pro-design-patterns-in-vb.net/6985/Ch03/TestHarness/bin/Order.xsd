<?xml version="1.0" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
	<xs:element name="Order">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Product" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Name" type="xs:string" minOccurs="0" msdata:Ordinal="0" />
							<xs:element name="Quantity" type="xs:positiveInteger" minOccurs="0" msdata:Ordinal="1" />
							<xs:element name="Price" type="xs:double" minOccurs="0" msdata:Ordinal="2" />
							<xs:element name="RequiredDate" type="xs:date" minOccurs="0" msdata:Ordinal="3" />
						</xs:sequence>
						<xs:attribute name="ProductID" form="unqualified" type="xs:positiveInteger" />
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="CustomerID" form="unqualified" type="xs:string" />
			<xs:attribute name="OrderDate" form="unqualified" type="xs:date" />
		</xs:complexType>
	</xs:element>
	<xs:element name="NewDataSet" msdata:IsDataSet="true" msdata:EnforceConstraints="False">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="Order" />
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>